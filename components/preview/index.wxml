<view class="container" catch:tap="onTap">
  <view class="head">
    <v-tag text="{{typeText}}" tag-class="tag" />
    <v-like like="{{true}}" class="like" read-only="{{true}}" like="{{true}}" count="{{classic.fav_nums}}" />
  </view>
  <image src="{{classic.image}}" class="{{classic.type==200?'music-img':'other-img'}}" />
  <view class="text">{{classic.content}}</view>
</view>